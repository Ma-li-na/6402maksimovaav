{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
      "simple": {
          "format": "%(asctime)s %(processName)-10s %(name)s %(levelname)-8s %(message)s"
      }
  },
  "handlers": {
      "consoleHandler": {
          "class": "logging.StreamHandler",
          "level": "WARNING",
          "formatter": "simple"
      },
      "file": {
          "class": "logging.FileHandler",
          "level": "WARNING",
          "formatter": "simple",
          "filename": "get_data.log",
          "mode": "a"
      }
  },
  "root": {
      "level": "WARNING",
      "handlers": [
          "consoleHandler",
          "file"
      ]
  }
}